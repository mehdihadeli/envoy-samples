{
    "name": "listener_2",
    "address": {
        "socket_address": {
            "address": "0.0.0.0",
            "port_value": 18123
        }
    },
    "filter_chains": [
        {
            "filters": [
                {
                    "name": "envoy.http_connection_manager",
                    "config": {
                        "stat_prefix": "ingress_http",
                        "codec_type": "auto",
                        "generate_request_id": true,
                        "route_config": {
                            "name": "local_http_router",
                            "virtual_hosts": [
                                {
                                    "name": "local_service",
                                    "domains": [
                                        "*"
                                    ],
                                    "routes": [
                                        {
                                            "match": {
                                                "prefix": "/"
                                            },
                                            "route": {
                                                "cluster": "app3"
                                            }
                                        }
                                    ]
                                }
                            ]
                        },
                        "http_filters": [
                            {
                                "name": "envoy.router"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}